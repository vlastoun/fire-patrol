{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "jest": true
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "arrow-parens": ["error", "always"],
    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": 0,
    "object-curly-spacing":"off",
    "import/prefer-default-export": 0,
    "react/jsx-tag-spacing": 0,
    "space-before-blocks":"off",
  }
}